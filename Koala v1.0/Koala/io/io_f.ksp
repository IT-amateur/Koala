function message_1(a)
	message(a)
end function

function message_2(a, b)
	message(a & " " & b)
end function

function message_3(a, b, c)
	message(a & " " & b & " " & c)
end function

function message_4(a, b, c, d)
	message(a & " " & b & " " & c & " " & d)
end function

function message_5(a, b, c, d, e)
	message(a & " " & b & " " & c & " " & d & " " & e)
end function

function message_6(a, b, c, d, e, f)
	message(a & " " & b & " " & c & " " & d & " " & e & " " & f)
end function

function message_7(a, b, c, d, e, f, g)
	message(a & " " & b & " " & c & " " & d & " " & e & " " & f & " " & g)
end function

function message_8(a, b, c, d, e, f, g, h)
	message(a & " " & b & " " & c & " " & d & " " & e & " " & f & " " & g & " " & h)
end function

function message_9(a, b, c, d, e, f, g, h, i)
	message(a & " " & b & " " & c & " " & d & " " & e & " " & f & " " & g & " " & h)
end function

function message_10(a, b, c, d, e, f, g, h, i, l)
	message(a & " " & b & " " & c & " " & d & " " & e & " " & f & " " & g & " " & h & " " & i)
end function

